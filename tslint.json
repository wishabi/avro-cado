{
  "defaultSeverity": "error",
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "jsRules": {},
  "rules": {
    "no-console": {
      "severity": "warn"
    },
    "indent": [true, "spaces"],
    "interface-name": [false],
    "member-ordering": [
      false,
      {
        "order": [
          "public-before-private",
          "static-before-instance",
          "variables-before-functions"
        ]
      }
    ],
    "object-literal-sort-keys": false,
    "no-any": false
  },
  "rulesDirectory": []
}
